{
  "autoload": {
    "psr-4": {
      "True\\OAuth2\\": "src"
    }
  },
  "require": {
    "league/oauth2-client": "^2.3",
    "lcobucci/jwt": "^3.2"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.13",
    "phpunit/phpunit": "^6.5",
    "cvo-technologies/stream-emulation": "^2.0"
  },
  "scripts": {
    "check": [
      "@php-cs-check"
    ],
    "fix": [
      "@php-cs-fix"
    ],
    "php-cs-check": "./vendor/bin/php-cs-fixer fix --allow-risky=yes --show-progress=run-in --dry-run",
    "php-cs-fix": "./vendor/bin/php-cs-fixer fix --allow-risky=yes --show-progress=run-in"
  }
}
